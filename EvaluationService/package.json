{
  "name": "expresstypescripttemplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev:api": "nodemon src/server.ts",
    "dev:submission-worker": "nodemon --exec ts-node src/workers/submission.worker.ts",
    "dev:leaderboard-worker": "nodemon --exec ts-node src/workers/leaderboard.worker.ts",
    "dev:all": "concurrently -n API,SUBMISSION,LEADERBOARD -c blue,green,yellow \"npm run dev:api\" \"npm run dev:submission-worker\" \"npm run dev:leaderboard-worker\""
  },
  "dependencies": {
    "bullmq": "^5.66.2",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.19.3",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}